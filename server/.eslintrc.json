{
  "root": true,
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
		"no-console": "off",
    "semi": [2, "always"],
    "no-useless-escape": "off",
    "no-unused-vars": "off"
  }
}
